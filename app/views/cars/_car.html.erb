<div id="<%= dom_id car %>">
  <p>
    <strong>Name:</strong>
    <%= car.name %>
  </p>

  <p>
    <strong>Price:</strong>
    <%= car.price %>
  </p>

  <p>
    <strong>Category:</strong>
    <%= car.category %>
 
  </p>

  <p>
    <strong>Model:</strong>
    <%= car.model %>
  </p>

  <p>
    <strong>Depreciation:</strong>
    <%= car.depreciation %>
  </p>

  <p>
  <strong>Dealership:</strong>
  <%= car.dealership.name %>
</p>

</div>

<div>
  <%= link_to "Back to cars", cars_path %>
  <br>
</div>
